{
  "name": "@swapparel/api",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile api",
    "dev": "bun run --env-file=../../.env --watch src/index.ts",
    "start": "bun run --env-file=../../.env src/index.ts",
    "lint": "biome check .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@elysiajs/cors": "^1.4.0",
    "@orpc/openapi": "^1.12.3",
    "@swapparel/contracts": "workspace:*",
    "better-auth": "^1.4.7",
    "dotenv": "^17.2.3",
    "elysia": "^1.4.19",
    "mongoose": "^8.20.2",
    "mongoose-zod": "github:ShreyasAyyengar/mongoose-zod#3a1b336f03233dfe83a2a7252415bbf27c0c29f1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "tsup": "^8.0.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "exports": {
    "./auth-server": "./src/libs/auth-server.ts"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "tsc-alias": "^1.8.16"
  }
}
