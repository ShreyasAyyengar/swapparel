{
  "name": "@swapparel/api",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile api",
    "dev": "bun run --env-file=../../.env --watch src/index.ts",
    "start": "bun run --env-file=../../.env src/index.ts",
    "lint": "biome check .",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@orpc/openapi": "^1.10.2",
    "@swapparel/contracts": "*",
    "@zodyac/zod-mongoose": "^4.2.0",
    "better-auth": "^1.3.31",
    "elysia": "^1.4.13",
    "mongoose": "^8.19.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}
